{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/RankLib/RankLib.csproj"
          ],
          "src": "../../"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [ ],
    "globalMetadata": {
      "_appName": "Search Pioneer",
      "_description": "RankLib for .NET is a .NET port of RankLib, a Learning to Rank library",
      "_appTitle": "RankLib for .NET",
      "_appLogoPath": "images/ranklib_dotnet.svg",
      "_appFaviconPath": "images/ranklib_dotnet_favicon.png",
      "_appFooter": "<span>Made by <a href=\"https://searchpioneer.com\"><img id=\"searchpioneer-logo\" src=\"https://searchpioneer.github.io/ranklib-dotnet/images/searchpioneer_logo.svg\" alt=\"Search Pioneer\"/></a></span>",
      "_lang": "en",
      "_enableSearch": true,
      "_googleAnalyticsTagId": "G-YRNZ8GQ0TR"
    },
    "sitemap": {
      "baseUrl": "https://searchpioneer.github.io/ranklib-dotnet/",
      "priority": 0.1,
      "changefreq": "daily"
    },
    "template": [
      "default",
      "modern",
      "template"
    ],
    "output": "_site",
    "disableGitFeatures": false
  }
}